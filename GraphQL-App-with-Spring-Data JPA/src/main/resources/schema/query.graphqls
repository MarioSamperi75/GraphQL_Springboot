type Query {
	firstQuery : String
	secondQuery : String
	fullName(sampleRequest : SampleRequest) : String
	student(id : Long) : StudentResponse
}

input SampleRequest {
	firstName : String!
	lastName : String
}

type StudentResponse{
	id : long
	firstName : String
	lastName : String
	email : String
	street : String
	city : String
	

}